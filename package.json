{
  "name": "router-js",
  "version": "3.9.1",
  "description": "A simple router for single-page web apps",
  "repository": {
    "type": "git",
    "url": "git://github.com/mkay581/router-js.git"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "author": {
    "name": "Mark Kennedy",
    "url": "http://heyimmark.com"
  },
  "engines": {
    "node": ">=8.9.0"
  },
  "keywords": [
    "router",
    "routing",
    "router element",
    "route management",
    "router component",
    "routing urls",
    "lit router",
    "polymer router"
  ],
  "license": "MIT",
  "main": "dist/router.js",
  "types": "src/router.ts",
  "devDependencies": {
    "@types/youtube": "0.0.34",
    "banner-cli": "0.11.1",
    "chai": "^4.1.2",
    "eslint": "^5.5.0",
    "karma": "^3.0.0",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-mocha": "^1.3.0",
    "karma-rollup-preprocessor": "^6.0.1",
    "mocha": "^5.2.0",
    "rollup": "^0.65.2",
    "rollup-plugin-node-resolve": "^3.4.0",
    "rollup-plugin-typescript": "^0.8.1",
    "sinon": "^6.2.0",
    "tslint": "^5.11.0",
    "typescript": "^3.0.3",
    "uglify-es": "^3.3.9"
  },
  "scripts": {
    "test": "karma start karma.conf.js && tslint src/**/*",
    "test-server": "karma start karma.conf.js --single-run=false",
    "preversion": "npm test",
    "minify": "uglifyjs dist/router.js -o dist/router.min.js",
    "build-dist": "rm -rf dist/* && rollup -c && npm run minify",
    "version": "npm run build-dist && banner-cli dist/*.js && git add -A dist",
    "postversion": "git push && git push --tags",
    "start-dev": "rollup -c --watch"
  }
}
